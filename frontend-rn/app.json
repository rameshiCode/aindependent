{
  "expo": {
    "name": "AIndependent",
    "slug": "AIndependent",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "aindependent",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.frontendrn"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://aindependent.com"
        }
      ],
      [
        "@stripe/stripe-react-native",
        {
          "publishableKey": "pk_test_51R04QIF7P8kDElAhIvVr0fDR3qSwPClQOSeTGaQhGAculMikrRATQaPFdF8G5chyz7ntVNOLAoC3A1weNRMDk4Qk00k20RqJ79"
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "API_URL": "http://localhost:8000",
      "router": {
        "origin": "https://aindependent.com"
      },
      "eas": {
        "projectId": "fd7e472d-31c9-445f-8c15-4f95c12e1391"
      }
    }
  }
}
